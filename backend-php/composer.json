{
  "name": "voc/backend-php",
  "description": "PHP 版后台服务，实现与 Java / Go 版相同的 API 与数据库结构，供前端统一复用。",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "slim/slim": "^4.13",
    "slim/psr7": "^1.6",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "Voc\\Admin\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  }
}

